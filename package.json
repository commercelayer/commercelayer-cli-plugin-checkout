{
  "name": "@commercelayer/cli-plugin-checkout",
  "description": "Commerce Layer CLI Checkout plugin",
  "version": "4.0.0",
  "author": "Pierluigi Viti <pierluigi@commercelayer.io>",
  "bugs": "https://github.com/commercelayer/commercelayer-cli-plugin-checkout/issues",
  "devDependencies": {
    "@oclif/dev-cli": "git+https://github.com/pviti/dev-cli.git",
    "@oclif/plugin-help": "^5.1.10",
    "@oclif/test": "^2.0.3",
    "@types/chai": "^4.2.22",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^16.11.0",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/commercelayer/commercelayer-cli-plugin-checkout",
  "keywords": [
    "oclif",
    "ecommerce",
    "cli",
    "commercelayer"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cl-checkout",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "plugins": [],
    "topics": {
      "checkout": {
        "description": "Generate checkout URLs",
        "hidden": false
      }
    },
    "repositoryPrefix": "<%- repo %>/blob/main/<%- commandPath %>",
    "additionalHelpFlags": [
      "-h"
    ],
    "topicSeparator": ":"
  },
  "repository": "commercelayer/commercelayer-cli-plugin-checkout",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && npm run readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "readme": "oclif-dev readme --plugin  && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@commercelayer/cli-core": "^1.3.13",
    "@commercelayer/js-auth": "^2.2.9",
    "@commercelayer/sdk": "^4.4.0",
    "@oclif/core": "^1.3.1",
    "open": "^8.4.0",
    "tslib": "^2.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
